int power (int arg0, int arg1) {
  int exponent_3 = arg1;
  int res_4 = 1;
  int x_5 = arg0;
  while (exponent_3 > 1) {
    if ((exponent_3 % 2) == 1) {
      res_4 = res_4 * x_5;
    } 
    x_5 = x_5 * x_5;
    exponent_3 = exponent_3 / 2;
  }
  return res_4 * x_5;
}

void bar (void) {
  int a_0;
  int x_1 = a_0 + 1;
  while (x_1) {
    a_0 = a_0 + 1;
  }
}

