int power (int arg0, int arg1) {
  int exponent_0 = arg1;
  int res_1 = 1;
  int x_2 = arg0;
  while (exponent_0 > 1) {
    if ((exponent_0 % 2) == 1) {
      res_1 = res_1 * x_2;
    } 
    x_2 = x_2 * x_2;
    exponent_0 = exponent_0 / 2;
  }
  return res_1 * x_2;
}

void bar (void) {
  int a_0;
  int x_1 = a_0 + 1;
  while (x_1) {
    a_0 = a_0 + 1;
  }
}

